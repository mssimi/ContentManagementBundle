{
    "name": "mssimi/content-management-bundle",
    "license": "MIT",
    "description": "Simple CMS",
    "authors": [
        {
            "name": "Marek Šimeček",
            "email": "mssimi@seznam.cz"
        }
    ],
    "autoload": {
        "psr-4": {
            "mssimi\\ContentManagementBundle\\": ""
        }
